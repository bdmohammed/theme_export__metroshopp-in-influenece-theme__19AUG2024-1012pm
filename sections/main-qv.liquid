                data-ts-thumb-true
                class="row ts-g-0 slide-eff-fade flickity {{image_ratio}} position_{{se_stts.image_position}} {{se_stts.image_size}}{% if se_stts.nav_btn %} flicky-slider slider-btn-{{se_stts.nav_btn}} slider-btn-style-{{se_stts.btn_owl}} slider-btn-{{se_stts.btn_shape}} slider-btn-{{se_stts.btn_size}} slider-btn-cl-{{se_stts.btn_cl}} slider-btn-vi-{{se_stts.btn_vi}} slider-btn-hidden-mobile-{{se_stts.btn_hidden_mobile}}{% endif %}"
